    "e2e": "ng e2e",
    "electron-build": "ng build --configuration=production && electron .",
    "electron": "electron --inspect .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --ignore=/node_modules --ignore=/e2e --ignore=/src --prune=true --out=release-builds",
