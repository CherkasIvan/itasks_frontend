  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-build": "ng build --configuration=production && electron .",
    "electron": "electron --inspect .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --ignore=/node_modules --ignore=/e2e --ignore=/src --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --ignore=/node_modules --ignore=/e2e --ignore=/src --prune=true --out=release-builds",
    "package-all": "ng build --configuration=production && npm run package-mac && npm run package-linux"
  },
